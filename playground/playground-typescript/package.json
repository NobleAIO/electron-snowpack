{
  "name": "playground-typescript",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "electron-snowpack build",
    "clean": "electron-snowpack clean",
    "dev": "electron-snowpack dev",
    "dist": "electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "try": "run-s clean build dist:dir; sleep 1; open dist/mac/${npm_package_name}.app"
  },
  "dependencies": {
    "electron-snowpack": "*"
  },
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/snowpack-env": "^2.3.3",
    "electron": "11.2.1",
    "electron-builder": "^22.9.1",
    "typescript": "^4.1.3"
  },
  "build": {
    "extends": "electron-snowpack/config/electron-builder.js"
  }
}
